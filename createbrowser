import socket
mysock=socket.socket(socket.AF_INET , socket.SOCK_STREAM)     # create socket
mysock.connect(("data_pride.org"),80)     #connect socket

cmd='GET http//data.pride.org/romeo.txt HTTP/1.0\n\n',encode()
while True:
  data=mysock.reev(512)
  if(len(data)<1):
      break
  print(data.encode())
  mysock.close()
  
  
